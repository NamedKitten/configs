[Configuration]
LastActiveMode=THUNAR_RENAMER_MODE_BOTH
LastActiveRenamer=ThunarSbrReplaceRenamer

[ThunarSbrRemoveRenamer]
EndOffsetMode=THUNAR_SBR_OFFSET_MODE_RIGHT
StartOffset=4
EndOffset=1
StartOffsetMode=THUNAR_SBR_OFFSET_MODE_RIGHT

[ThunarSbrReplaceRenamer]
Regexp=FALSE
Replacement=.mp4
CaseSensitive=FALSE
Pattern=.avi.mp4

