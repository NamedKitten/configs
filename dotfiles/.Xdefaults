urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c800
*foreground: #f8f6f2
*cursorColor: #aeee00
*scrollColor: #45413b
*fadeColor: #ffffff
*fading: 0

! black
*color0: #141413
*color8: #000000
! red
*color1: #ff2c4b
*color9: #ff2c4b
! green
*color2: #aeee00
*color10: #aeee00
! yellow
*color3: #fade3e
*color11: #ffa724
! blue
*color4: #0a9dff
*color12: #0a9dff
! magenta
*color5: #ff9eb8
*color13: #ff9eb8
! cyan
*color6: #8cffba
*color14: #8cffba
! white
*color7: #d9cec3
*color15: #ffffff
urxvt*font:xft:Noto Mono:pixelsize=10
urxvt*boldfont:xft:Noto Mono:pixelsize=10
urxvt.scrollBar: false

! 
------------------------------------------------------------------------------
! ROFI Color theme
! 
------------------------------------------------------------------------------
rofi.color-enabled: true
!rofi.color-window: #1c1b1a, #1c1b1a, #1c1b1a
!rofi.color-normal: #1c1b1a, #f8f6f2, #1c1b1a, #1c1b1a, #aeee00
rofi.color-active: #1c1b1a, #f8f6f2, #1c1b1a, #1c1b1a, #aeee00
rofi.color-urgent: #1c1b1a, #f8f6f2, #1c1b1a, #1c1b1a, #aeee00
!rofi.color-window: argb: EE000000, argb: EE000000, argb: EE000000
!rofi.color-normal: argb: 00000000, #DEDEDE, argb: 00000000, #DEDEDE
rofi.color-window: argb:ee000000, argb:ee000000, argb:ee000000
rofi.color-normal: argb:0000000, #dedede, argb:0000000, #1c1b1a, #aeee00
